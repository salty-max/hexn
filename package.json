{
	"name": "hexn",
	"version": "0.2.0",
	"description": "A CLI to mess with bytes",
	"main": "./bin/index.js",
	"bin": {
		"hexn": "./bin/index.js"
	},
	"repository": "git@github.com:salty-max/hexn.git",
	"author": "Maxime Blanc <max@jellycat.fr>",
	"license": "MIT",
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"prebuild": "yarn clean",
		"build": "tsc",
		"postbuild": "node ./scripts/add-shebang.js && chmod +x ./bin/index.js",
		"dev": "tsc --watch",
		"hexn": "node bin --file example.bin",
		"gen": "node bin/generate-bin.js 512 example.bin",
		"lint": "eslint src",
		"lint:fix": "eslint src --fix",
		"fmt": "prettier --write src",
		"clean": "rimraf bin",
		"prepack": "npm run build"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"ink": "^4.1.0",
		"meow": "^11.0.0",
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/node": "^20.4.5",
		"@types/react": "^18.0.32",
		"@types/sinon": "^10.0.15",
		"@typescript-eslint/eslint-plugin": "^6.2.0",
		"@typescript-eslint/parser": "^6.2.0",
		"@vdemedes/prettier-config": "^2.0.1",
		"chalk": "^5.2.0",
		"eslint": "^8.45.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.33.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"fs-extra": "^11.1.1",
		"ink-testing-library": "^3.0.0",
		"prettier": "^2.8.7",
		"rimraf": "^5.0.1",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.3"
	},
	"prettier": "@vdemedes/prettier-config"
}